  "repository": {
    "type": "git",
    "url": "https://github.com/microfleet/ioredis-lock.git"
  },
  "dependencies": {
    "uuid": "^11.0.5"
  },
  "peerDependencies": {
    "ioredis": "~4.x.x || ~5.x.x"
  },
  "devDependencies": {
    "@makeomatic/deploy": "^13.1.0",
    "@oxc-resolver/binding-linux-arm64-musl": "^3.0.3",
